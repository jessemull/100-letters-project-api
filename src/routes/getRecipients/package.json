{
  "dependencies": {
    "aws-sdk": "^2.1360.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "typescript": "^5.2.2"
  },
  "main": "dist/index.js",
  "name": "getRecipients",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc && npm run postbuild",
    "postbuild": "cp package.json dist/ && [ -f package-lock.json ] && cp package-lock.json dist/ || true && cd dist && npm install --omit=dev",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "package": "cd dist && zip -r getRecipients.zip ./*"
  },
  "version": "1.0.0"
}
